<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/secondary_orange_600" />
            <stroke android:width="1dp" android:color="@color/secondary_orange_600" />
            <corners android:bottomLeftRadius="@dimen/base_margin_small" android:bottomRightRadius="@dimen/base_margin_small" android:topLeftRadius="@dimen/base_margin_small" android:topRightRadius="@dimen/base_margin_small" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/gray_300" />
            <stroke android:width="1dp" android:color="@color/gray_200" />
            <corners android:bottomLeftRadius="@dimen/base_margin_small" android:bottomRightRadius="@dimen/base_margin_small" android:topLeftRadius="@dimen/base_margin_small" android:topRightRadius="@dimen/base_margin_small" />
        </shape>
    </item>
</selector>